#include<iostream>
using namespace std;
#include<iomanip>

int main()
{
    const int SIZE = 10;
    int arr[SIZE]  = {9,6,4,8,10,12,89,68,45,37};
    
    for (int i=0;i<SIZE-1;i++)
    {
        for(int j =0;j<SIZE-1;j++)
        {
            if (arr[j]>arr[j+1])
            {
                int temp = arr[j+1];
                arr[j+1] = arr[j];
                arr[j]  = temp;
            }
        }
    }
    for (int k = 0; k<SIZE;k++)
    {
        cout<<arr[k]<<"  ";
    }
    return 0;
}


Optimized
#include<iostream>
using namespace std;
#include<iomanip>

int main()
{
    const int SIZE = 10;
    int arr[SIZE]  = {9,6,4,8,10,12,89,68,45,37};
    
    for (int i=0;i<SIZE-1;i++)
    {
        for(int j =0;j<SIZE-1-i;j++)
        {
            if (arr[j]>arr[j+1])
            {
                int temp = arr[j+1];
                arr[j+1] = arr[j];
                arr[j]  = temp;
            }
        }
    }
    for (int k = 0; k<SIZE;k++)
    {
        cout<<arr[k]<<"  ";
    }
    return 0;
}
